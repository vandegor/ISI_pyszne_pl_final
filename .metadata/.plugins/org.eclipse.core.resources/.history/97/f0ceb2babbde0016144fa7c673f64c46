package xmlObjects;

import javax.xml.bind.annotation.XmlElement;

@XmlRootElement(name = "pozycjazamowienia")
@XmlAccessorType(XmlAccessType.FIELD)
public class PozycjaZamowieniaXml {

	@XmlElement(name = "id")
	public Integer id;

	@XmlElement(name = "nrPozycji")
	public Integer nrPozycji;

	@XmlElement(name = "produkt")
	public ProduktXml produkt;

	@XmlElement(name = "ilosc")
	public Integer ilosc;

	@XmlElement(name = "cenaJednostkowa")
	public Float cenaJednostkowa;

}
